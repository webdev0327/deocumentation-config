{
	"name": "@barchart/documentation-generator",
	"version": "1.0.0",
	"description": "Generates standardized documentation sites for Barchart products",
	"scripts": {
		"docs": "lib/docsify serve ./docs",
		"prettier": "prettier --config .prettierrc --write './**/*.js'",
		"lint": "eslint --fix --config=.eslintrc './**/*.js'"
	},
	"bin": {
		"barchart-documentation": "bin/cli.js"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/barchart/documentation-generator.git"
	},
	"author": {
		"name": "Bryan Ingle",
		"email": "bryan.ingle@barchart.com",
		"url": "http://www.barchart.com"
	},
	"bugs": {
		"url": "https://github.com/barchart/documentation-generator/issues"
	},
	"homepage": "https://github.com/barchart/documentation-generator#readme",
	"dependencies": {
		"array-back": "^4.0.1",
		"commander": "^5.1.0",
		"compare-versions": "^3.6.0",
		"docsify-cli": "^4.4.0",
		"fast-safe-stringify": "^2.0.7",
		"file-set": "^3.0.0",
		"handlebars": "^4.7.6",
		"inquirer": "^7.1.0",
		"jsdoc-to-markdown": "^5.0.3",
		"marked": "^0.7.0",
		"object-get": "^2.1.0",
		"openapi-sampler": "^1.0.0-beta.15",
