		"test-value": "^3.0.0"
	},
	"devDependencies": {
		"eslint": "^6.5.1",
		"eslint-config-prettier": "^6.4.0",
		"eslint-plugin-prettier": "^3.1.1",
		"eslint-plugin-sort-requires": "^2.1.0",
		"prettier": "^1.18.2"
